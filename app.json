{
  "expo": {
    "name": "Tiangero",
    "slug": "community-marketplace",
    "scheme": "community-marketplace",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/play_store_512.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/tiangerologo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.banderadigital.tiangero",
      "usesAppleSignIn": true
    },
    "android": {
      "package": "com.banderadigital.communitymarketplace",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/play_store_512.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/images/tiangerologo.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "La aplicación permite compartir las fotos de tu producto para compartir en línea."
        }
      ],
      "expo-secure-store",
      "expo-apple-authentication"
    ],
    "extra": {
      "eas": {
        "projectId": "e12550d4-3ec9-4854-b079-5d3e179af5a5"
      }
    }
  }
}
