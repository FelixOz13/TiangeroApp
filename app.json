{
  "expo": {
    "name": "community-marketplace",
    "slug": "community-marketplace",
     "scheme": "community-marketplace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/play_store_512.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/tiangerologo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
      
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.banderadigital.communitymarketplace",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/play_store_512.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/tiangerologo.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "La applicacion permite compartir las fotos de tu producto para compartir en linea."
        }
      ]
    ]
  }
}
